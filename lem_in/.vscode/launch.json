    "version": "0.2.0",
    "configurations": [
        {
            "name": "(cppdbg) launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/lem_in",
            "args": ["testmap_copy"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/lem_in",
            "args": ["testmap_copy3"],
            "cwd": "${workspaceFolder}"
        }
